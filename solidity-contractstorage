//CONTRACT//
contract Simple {
    function(uint _a, uint _b) returns (uint o_product, uint o_sum){
        o_sum = _a + _b;
        o_product = _a * _b;
    }
}
//STORAGE//
contract SimpleStorage {
  uint storedData;
  
  function set(uint x){
    storedData = x;
    }
    
    function get() constant returns (uint retVal){
      return storedData;
  }
}

